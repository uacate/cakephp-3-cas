{
    "name": "snelg/cakephp-3-cas",
    "description": "CAS Authentication for CakePHP 3",
    "kewords": ["CAS", "authentication"],
    "license": "MIT",
    "type": "package",
    "minimum-stability": "dev",
    "require": {
        "cakephp/cakephp": "^3.5.0",
        "jasig/phpcas": "~1.3.3"
    },
    "require-dev": {
        "psr/log": "~1.0",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "CasAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CasAuth\\Test\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:uazgraduatecollege/cakephp-3-cas.git"
        }
    ]
}

